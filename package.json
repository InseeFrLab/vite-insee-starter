{
    "name": "vite-insee-starter",
    "private": true,
    "repository": "https://github.com/InseeFrLab/vite-insee-starter.git",
    "license": "MIT",
    "version": "2.2.5",
    "type": "module",
    "scripts": {
        "postinstall": "vite-envs update-types",
        "dev": "vite --host",
        "build": "tsc && vite build",
        "lint": "eslint ./src",
        "preview": "vite preview",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "predev": "react-dsfr update-icons",
        "prebuild": "react-dsfr update-icons",
        "generate-todos-api-client": "tsx scripts/generate-todos-api-client.ts",
        "prepare": "husky"
    },
    "dependencies": {
        "@codegouvfr/react-dsfr": "^1.16.10",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.4.1",
        "@mui/material": "^6.4.1",
        "@mui/system": "^6.4.1",
        "@mui/x-data-grid": "^7.24.0",
        "@mui/x-date-pickers": "^7.24.0",
        "@tanstack/react-query": "^5.64.2",
        "@tanstack/react-router": "^1.97.14",
        "axios": "^1.7.9",
        "dayjs": "^1.11.13",
        "i18nifty": "^3.2.3",
        "oidc-spa": "^6.1.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tsafe": "^1.8.5",
        "tss-react": "^4.9.15",
        "vite-envs": "^4.4.10",
        "vite-tsconfig-paths": "^5.1.4",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.18.0",
        "@tanstack/router-devtools": "^1.97.14",
        "@tanstack/router-vite-plugin": "^1.97.14",
        "@types/node": "^22.10.10",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.18.0",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.18",
        "eslint-plugin-tss-unused-classes": "^1.0.3",
        "globals": "^15.14.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.2",
        "node-fetch": "^3.3.2",
        "orval": "^7.4.1",
        "prettier": "^3.4.2",
        "tsx": "^4.19.2",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.21.0",
        "vite": "^6.0.11"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    }
}
