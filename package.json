{
    "name": "vite-insee-starter",
    "private": true,
    "repository": "https://github.com/InseeFrLab/vite-insee-starter.git",
    "license": "MIT",
    "version": "2.4.15",
    "type": "module",
    "scripts": {
        "postinstall": "vite-envs update-types",
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "eslint ./src",
        "preview": "vite preview",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "npm run _format -- --write",
        "format:check": "npm run _format -- --list-different",
        "predev": "react-dsfr update-icons",
        "prebuild": "react-dsfr update-icons",
        "generate-todos-api-client": "tsx scripts/generate-todos-api-client.ts",
        "prepare": "husky"
    },
    "dependencies": {
        "@codegouvfr/react-dsfr": "^1.23.2",
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@mui/icons-material": "^6.4.8",
        "@mui/material": "^6.4.8",
        "@mui/system": "^6.4.1",
        "@mui/x-data-grid": "^7.24.0",
        "@mui/x-date-pickers": "^7.24.0",
        "@tanstack/react-query": "^5.69.0",
        "@tanstack/react-router": "^1.114.27",
        "axios": "^1.8.4",
        "dayjs": "^1.11.13",
        "i18nifty": "^3.2.6",
        "oidc-spa": "^6.13.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "tsafe": "^1.8.5",
        "tss-react": "^4.9.15",
        "vite-envs": "^4.6.0",
        "vite-tsconfig-paths": "^5.1.4",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@tanstack/router-devtools": "^1.114.27",
        "@tanstack/router-vite-plugin": "^1.114.27",
        "@types/node": "^22.13.11",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.23.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "eslint-plugin-tss-unused-classes": "^1.0.3",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.4.2",
        "node-fetch": "^3.3.2",
        "orval": "^7.4.1",
        "prettier": "^3.5.3",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0",
        "vite": "^6.2.2"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix"
        ],
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    }
}
